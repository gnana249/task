pipeline 
     {
      agent any
      stages
      {
       stage('git')
       {
        steps
            {
              git "https://github.com/gnana249/task.git"
              }
}
        stage('run')
        {
         steps
            {
             sh "java Demo.java"
             sh "main.py"
             }
        }
       
      }//stages
      }//pipeline
